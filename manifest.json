{
  "manifest_version": 3,
  "name": "Family reunion helper",
  "version": "1.0.2",
  "action": { "default_popup": "popup.html" },
  "permissions": ["storage"],
  "content_scripts": [{
    "matches": ["https://service2.diplo.de/rktermin/extern*"],
    "js": ["content.js"],
    "run_at": "document_end",
    "all_frames": true
  }],
  "icons": {
    "16": "icons/ai.png",
    "48": "icons/ai.png",
    "128": "icons/ai.png"
  }
}
